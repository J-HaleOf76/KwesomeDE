<schemalist>
  <schema id="org.awesome.settings" path="/org/awesome/settings/">
    <key name="theme-colorschemes" type="a{sas}">
      <default>{"~/.config/awesome/ui/assets/wallpapers/52_by_gydw1n_dblxpwi.jpg":[
       "#010610",
       "#2C5F9A",
       "#2972E4",
       "#4C7ADD",
       "#488CB1",
       "#3493ED",
       "#549CED",
       "#A7E0F2",
       "#04183F",
       "#1579ED",
       "#5F9DFF",
       "#84ABFE",
       "#5FB6E5",
       "#79BEFF",
       "#A2CEFF",
       "#D3EFF8"]}</default>
      <summary>Colorschemes</summary>
      <description>
        All the generated colorschemes
      </description>
    </key>

    <key name="theme-active-colorscheme" type="s">
      <default>"~/.config/awesome/ui/assets/wallpapers/52_by_gydw1n_dblxpwi.jpg"</default>
      <summary>Active colorscheme</summary>
      <description>
        The active colorscheme
      </description>
    </key>

    <key name="theme-active-wallpaper" type="s">
      <default>"~/.config/awesome/ui/assets/wallpapers/52_by_gydw1n_dblxpwi.jpg"</default>
      <summary>Active wallpaper</summary>
      <description>
        The active wallpaper
      </description>
    </key>

    <key name="theme-wallpaper-type" type="s">
      <default>"image"</default>
      <summary>Wallpaper type</summary>
      <description>
        The wallpaper type (image/sun/binary/etc)
      </description>
    </key>

    <key name="theme-command-after-generation" type="s">
      <default>""</default>
      <summary>Command after generation</summary>
      <description>
        Command to run after colorscheme change
      </description>
    </key>

    <key name="theme-ui-opacity" type="s">
      <default>'0.4'</default>
      <summary>UI opacity</summary>
      <description>
        The opacity of AwesomeWM UI elements
      </description>
    </key>

    <key name="theme-ui-border-radius" type="s">
      <default>'36'</default>
      <summary>UI border raidus</summary>
      <description>
        The border radius of AwesomeWM UI elements
      </description>
    </key>

    <key name="theme-dpi" type="s">
      <default>'96'</default>
      <summary>UI DPI</summary>
      <description>
        Scale of UI elements
      </description>
    </key>

    <key name="theme-client-gap" type="s">
      <default>'0'</default>
      <summary>Client gap</summary>
      <description>
        The gap between the screen and the clients
      </description>
    </key>

    <key name="theme-useless-gap" type="s">
      <default>'0'</default>
      <summary>Useless gap</summary>
      <description>
        The gap between the clients
      </description>
    </key>

    <key name="theme-ui-animations" type="b">
      <default>true</default>
      <summary>UI animations</summary>
      <description>
        Enable or disable UI animations
      </description>
    </key>

    <key name="theme-ui-animations-framerate" type="s">
      <default>"144"</default>
      <summary>UI animations framerate</summary>
      <description>
        The framerate of the UI animations
      </description>
    </key>

   <key name="version" type="s">
      <default>""</default>
      <summary>Version</summary>
      <description>
        KwesomeDE version
      </description>
    </key>

    <key name="need-setup" type="b">
      <default>true</default>
      <summary>Need Setup</summary>
      <description>
        If true the welcome screen will show
      </description>
    </key>

    <key name="airplane" type="b">
      <default>false</default>
      <summary>Airplane mode</summary>
      <description>
        Enable or disable Airplane mode
      </description>
    </key>

    <key name="redshift" type="b">
      <default>false</default>
      <summary>Redshift</summary>
      <description>
        Enable or disable Redshift
      </description>
    </key>

    <key name="dont-disturb" type="b">
      <default>false</default>
      <summary>Don't Disturb</summary>
      <description>
        Disable notifications when enabled
      </description>
    </key>

    <key name="password" type="s">
      <default>""</default>
      <summary>Password</summary>
      <description>
        The lockscreen password (not secured)
      </description>
    </key>

    <key name="screenshot-show-cursor" type="b">
      <default>true</default>
      <summary>Screenshot show cursor</summary>
      <description>
        Should the cursor show in the screenshot
      </description>
    </key>

    <key name="screenshot-delay" type="i">
      <default>1</default>
      <summary>Screenshot delay</summary>
      <description>
        How much time to wait before taking the screenshot
      </description>
    </key>

    <key name="screenshot-folder" type="s">
      <default>"~/"</default>
      <summary>Screenshot folder</summary>
      <description>
        Where to save the screenshots
      </description>
    </key>


    <key name="record-resolution" type="s">
      <default>"1920x1080"</default>
      <summary>Record resolution</summary>
      <description>
        The resolution of the recording
      </description>
    </key>

    <key name="record-fps" type="i">
      <default>60</default>
      <summary>Record FPS</summary>
      <description>
        The recording frame rate
      </description>
    </key>

    <key name="record-delay" type="i">
      <default>0</default>
      <summary>Record delay</summary>
      <description>
        How much time to wait before starting to record
      </description>
    </key>

   <key name="record-folder" type="s">
      <default>"~/"</default>
      <summary>Record folder</summary>
      <description>
        The file format of the recording
      </description>
    </key>

    <key name="record-format" type="s">
      <default>"mp4"</default>
      <summary>Record format</summary>
      <description>
        The file format of the recording
      </description>
    </key>

    <key name="github-username" type="s">
      <default>""</default>
      <summary>Github username</summary>
      <description>
        Your Github username
      </description>
    </key>

    <key name="gitlab-host" type="s">
      <default>"https://gitlab.com"</default>
      <summary>Gitlab host</summary>
      <description>
        Your Gitlab host URL
      </description>
    </key>

    <key name="gitlab-access-token" type="s">
      <default>""</default>
      <summary>Gitlab access token</summary>
      <description>
        Your Gitlab access token
      </description>
    </key>

     <key name="weather-api-key" type="s">
      <default>""</default>
      <summary>Weather API key</summary>
      <description>
        Your OpenWeather API key
      </description>
    </key>

    <key name="weather-latitude" type="s">
      <default>"40.730610"</default>
      <summary>Weather latitude</summary>
      <description>
        The latitude at your location
      </description>
    </key>

    <key name="weather-longitude" type="s">
      <default>"-73.935242"</default>
      <summary>Weather longitude</summary>
      <description>
        The Longitude at your location
      </description>
    </key>

    <key name="weather-unit" type="s">
      <choices>
        <choice value="metric"/>
        <choice value="imperial"/>
      </choices>
      <default>"metric"</default>
         <summary>Weather unit</summary>
      <description>
        The measurement systems to use
      </description>
    </key>

    <key name="favorite-apps" type="aa{ss}">
      <default>[]</default>
      <summary>Favorite apps</summary>
      <description>
        Your most loved apps
      </description>
    </key>

    <key name="picom" type="b">
      <default>true</default>
      <summary>Picom</summary>
      <description>
        Enable or disable picom
      </description>
    </key>

    <key name="picom-shadow" type="b">
      <default>true</default>
      <summary>Picom shadow</summary>
      <description>
        Enable or disable picom shadows
      </description>
    </key>


    <key name="picom-fading" type="b">
      <default>true</default>
      <summary>Picom fading</summary>
      <description>
        Enable or disable picom fading
      </description>
    </key>


    <key name="picom-animations" type="b">
      <default>false</default>
      <summary>Picom animations</summary>
      <description>
        Enable or disable picom animations
      </description>
    </key>

    <key name="picom-animation-stiffness" type="s">
      <default>"200"</default>
      <summary>Picom animation stiffness</summary>
      <description>
        Stiffness (a.k.a. tension) parameter for spring-based animation
      </description>
    </key>

    <key name="picom-animation-dampening" type="s">
      <default>"25"</default>
      <summary>Picom animation dampening</summary>
      <description>
        Dampening (a.k.a. friction) parameter for spring-based animation
      </description>
    </key>

    <key name="picom-animation-window-mass" type="s">
      <default>"1"</default>
      <summary>Picom animation window mass</summary>
      <description>
        Mass parameter for spring-based animation
      </description>
    </key>

   <key name="picom-animation-clamping" type="b">
      <default>true</default>
      <summary>Picom animation clamping</summary>
      <description>
        Whether to clamp animations
      </description>
    </key>

   <key name="picom-active-opacity" type="s">
      <default>"0.9"</default>
      <summary>Picom active opacity</summary>
      <description>
        The opacity of the currently focused client
      </description>
    </key>

   <key name="picom-inactive-opacity" type="s">
      <default>"0.5"</default>
      <summary>Picom inactive opacity</summary>
      <description>
        The opacity of inactive clients
      </description>
    </key>

  <key name="picom-shadow-radius" type="s">
      <default>"12"</default>
      <summary>Picom shadow radius</summary>
      <description>
        The blur radius for shadows
      </description>
    </key>

   <key name="picom-shadow-offset-x" type="s">
      <default>"-15"</default>
      <summary>Picom shadow offset x</summary>
      <description>
        The left offset for shadows
      </description>
    </key>

    <key name="picom-shadow-offset-y" type="s">
      <default>"-15"</default>
      <summary>Picom shadow offset y</summary>
      <description>
        The top offset for shadows, in pixels
      </description>
    </key>

   <key name="picom-shadow-opacity" type="s">
      <default>"0.75"</default>
      <summary>Picom shadow opacity</summary>
      <description>
        The translucency for shadows
      </description>
    </key>

   <key name="picom-fade-delta" type="s">
      <default>"10"</default>
      <summary>Picom fade delta</summary>
      <description>
        The time between steps in a fade in milliseconds
      </description>
    </key>

   <key name="picom-fade-in-step" type="s">
      <default>"0.028"</default>
      <summary>Picom fade in step</summary>
      <description>
        Opacity change between steps while fading in
      </description>
    </key>

   <key name="picom-fade-out-step" type="s">
      <default>"0.03"</default>
      <summary>Picom fade out sterp</summary>
      <description>
        Opacity change between steps while fading out
      </description>
    </key>

   <key name="picom-corner-radius" type="s">
      <default>"0"</default>
      <summary>Picom corner radius</summary>
      <description>
        How much rounding to apply to the clients
      </description>
    </key>

   <key name="picom-blur-strength" type="s">
      <default>"10"</default>
      <summary>Picom blur</summary>
      <description>
        Blur amount
      </description>
    </key>

  </schema>
</schemalist>